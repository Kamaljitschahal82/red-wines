stat_lines <- function() {
lines <- geom_vline(aes(xintercept=mean(fixed.acidity)), color="red", linetype="dashed", size=1) +
geom_vline(aes(xintercept=median(fixed.acidity)), color="blue", size=1) +
geom_vline(aes(xintercept=quantile(fixed.acidity,0.25)), color="green", linetype="dotted", size=1) +
geom_vline(aes(xintercept=quantile(fixed.acidity,0.75)), color="green", linetype="dotted", size=1)
return(lines)
}
