jarl <- function(df, var) {return(mean(df[,var]))}
jarl(reds, "id")
stat_lines <- function(df, var) {
lines <- geom_vline(aes(xintercept=mean(df[,var])), color="red", linetype="dashed", size=1)
return(lines)
}
p <- ggplot(data = reds, aes(x = fixed.acidity)) +
geom_histogram(binwidth = 0.5) +
scale_x_continuous(breaks = seq(4, 16, 1), limits = c(4, 16)) +
stat_lines(reds, "fixed.acidity")
print(p)
